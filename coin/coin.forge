
for _, cc in toolsets('cc.*') do
    local cc = cc:inherit 
    {
        include_directories = 
        {
            '../'
        };
    };

    cc:StaticLibrary '${lib}/coin' 
    {
        cc:Cxx '${obj}/%1' 
        {
            'log.cpp',
            'file.cpp',
            'thread/Pool.cpp'
        };
    };
end
